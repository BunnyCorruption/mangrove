{
  "name": "mangrove-server",
  "version": "1.0.0",
  "description": "Data processing and visualization software for soundscape ecology research",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanbeever/mangrove"
  },
  "license": "GPLv3",
  "contributors": [
    "Ot Gabaldon",
    "Keith Guske",
    "David Palumbo",
    "Josh Pollmann",
    "Brita Ramsay"
  ],
  "main": "server.js",
  "scripts": {
    "pretest": "node ./scripts/makeUser.js && eslint --ignore-path ../.gitignore .",
    "test": "cross-env-shell NODE_ENV=test nyc --reporter=text --reporter=html mocha",
    "lint-fix": "eslint --ignore-path ../.gitignore --fix .",
    "predev": "node ./scripts/env.js",
    "dev": "cross-env-shell NODE_ENV=dev nodemon server.js",
    "prestart": "node ./scripts/env.js",
    "start": "node server.js",
    "predocker": "run-script-os",
    "predocker:darwin:linux": "MANGROVE_HOME=`node ./scripts/env.js rootDir` MANGROVE_INPUT_DIR=`node ./scripts/env.js inputDir`",
    "predocker:win32": "for /F %B in ('node ./scripts/env.js inputDir') do cross-env-shell MANGROVE_INPUT_DIR=%B cross-env-shell MANGROVE_HOME=%USERPROFILE%\\.mangrove",
    "docker-test": "npm run predocker cross-env-shell CMD=test docker-compose up",
    "docker-dev": "npm run predocker cross-env-shell CMD=dev docker-compose up",
    "docker-start": "npm run predocker cross-env-shell CMD=start docker-compose up",
    "docker-clean": "run-script-os",
    "docker-clean:darwin:linux": "docker-compose down; docker image rm server_mangrove",
    "docker-clean:win32": "docker-compose down & docker image rm server_mangrove"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-fs": "2.0.0",
    "chai-http": "4.2.1",
    "eslint": "5.15.3",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.16.0",
    "mocha": "6.0.2",
    "mockgoose": "8.0.1",
    "nodemon": "1.18.10",
    "nyc": "13.3.0"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "3.0.11",
    "body-parser": "1.18.3",
    "bull": "3.7.0",
    "config": "3.0.1",
    "cors": "2.8.5",
    "cross-env": "5.2.0",
    "execa": "1.0.0",
    "express": "4.16.4",
    "ffprobe-static": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.1",
    "mkdirp": "0.5.1",
    "moment": "2.24.0",
    "mongoose": "5.4.20",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "node-fetch": "2.3.0",
    "r-script": "git+https://git@github.com/khamurdik/r-script.git",
    "rimraf": "2.6.3",
    "run-script-os": "1.0.3",
    "settings-store": "1.4.1"
  }
}
