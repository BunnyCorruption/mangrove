{
  "name": "SoundScape",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.4.0",
    "@material-ui/icons": "3.0.1",
    "axios": "0.18.0",
    "concurrently": "4.0.1",
    "d3-array": "2.0.2",
    "d3-axis": "1.0.12",
    "d3-brush": "1.0.6",
    "d3-geo": "1.11.1",
    "d3-scale": "2.1.2",
    "d3-selection": "1.3.2",
    "d3-shape": "1.2.2",
    "d3-svg-legend": "2.25.6",
    "d3-transition": "1.1.3",
    "electron": "3.0.7",
    "electron-builder": "20.31.2",
    "electron-is-dev": "1.0.1",
    "file-saver": "^2.0.0",
    "lodash": "4.17.11",
    "moment": "2.23.0",
    "react": "16.6.0",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.6.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.1",
    "recharts": "1.4.1",
    "save-file": "^2.2.1",
    "styled-components": "4.1.3",
    "top-bar-nav": "1.2.2",
    "wait-on": "3.1.0",
    "yarn": "1.12.1"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3001 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "com.example.electron-test",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron-devtools-installer": "2.2.4"
  }
}
