{
  "name": "mangrove-client",
  "version": "1.0.0",
  "description": "Data processing and visualization software for soundscape ecology research",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonathanbeever/mangrove"
  },
  "license": "GPLv3",
  "contributors": [
    "Ot Gabaldon",
    "Keith Guske",
    "David Palumbo",
    "Josh Pollmann",
    "Brita Ramsay"
  ],
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "amazon-cognito-identity-js": "3.0.9",
    "axios": "0.18.0",
    "concurrently": "4.1.0",
    "d3-array": "2.0.3",
    "d3-axis": "1.0.12",
    "d3-brush": "1.0.6",
    "d3-geo": "1.11.3",
    "d3-scale": "3.0.0",
    "d3-selection": "1.4.0",
    "d3-shape": "1.3.5",
    "d3-svg-legend": "2.25.6",
    "d3-transition": "1.2.0",
    "electron": "4.1.1",
    "electron-builder": "20.39.0",
    "electron-is-dev": "1.0.1",
    "file-saver": "2.0.1",
    "json2csv": "4.4.1",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.1",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "npm-check-updates": "3.1.2",
    "react": "16.8.5",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.8.5",
    "react-player": "1.10.0",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "recharts": "1.5.0",
    "run-script-os": "1.0.5",
    "save-file": "2.3.1",
    "styled-components": "4.2.0",
    "top-bar-nav": "1.2.2",
    "wait-on": "3.2.0",
    "yarn": "1.15.2"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "run-script-os",
    "start:darwin:linux": "export PORT=3001 && react-scripts start",
    "start:win32": "set PORT=3001&&react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "run-script-os",
    "electron-dev:darwin:linux": "concurrently \"export BROWSER=none && npm run start\" \"wait-on http://localhost:3001 && electron .\"",
    "electron-dev:win32": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3001 && electron .\"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "com.example.electron-test",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron-devtools-installer": "2.2.4"
  }
}
